
import kotlin.Int;

CREATE TABLE Interview (
   id INTEGER AS Int PRIMARY KEY AUTOINCREMENT,
   show_text TEXT,
   left_number TEXT,
   right_number TEXT,
   result TEXT,
   create_time INTEGER
);

getAllHistory:
SELECT * FROM Interview;

insertHistory:
INSERT INTO Interview(show_text, left_number, right_number, result, create_time)
VALUES ?;

deleteHistory:
DELETE FROM Interview WHERE Interview.id == ?;

deleteAllHistory:
DELETE FROM Interview;
